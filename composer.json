{
    "name": "abellion/xenus-laravel",
    "description": "Xenus integration for the Laravel and Lumen frameworks",
    "keywords": ["mongodb", "laravel", "lumen", "odm", "xenus"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1.3|^8.0",
        "abellion/xenus": "^0.18.0",
        "illuminate/contracts": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/queue": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/database": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "illuminate/container": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "phpunit/phpunit": "^7.5|^8.0|^9.0",
        "illuminate/config": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/events": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Xenus\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xenus\\Laravel\\Tests\\": "tests/"
        }
    }
}
